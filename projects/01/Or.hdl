 /**
 * Or gate:
 * out = 1 if (a == 1 or b == 1)
 *       0 otherwise
 */

CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    Not(in=b, out=outNotB);
    Nand(a=a, b=outNotB, out=outNandANotB);
    Nand(a=outNotB, b=outNandANotB, out=out);
}
